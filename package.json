{
  "name": "angular-news-webapp",
  "version": "1.1.0",
  "description": "Angular News",
  "author": "Steve Martin <steven_martin@me.com>",
  "license": "MIT",
  "keywords": [
    "news",
    "angular"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/steven-martin/angular-news-webapp"
  },
  "scripts": {
    "ng": "ng",
    "postinstall": "ngcc",
    "start": "ng serve -o",
    "start:prod": "ng serve --prod -o",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "test:watch": "ng test --watch=true",
    "test:headless": "ng test --browsers=ChromeHeadless",
    "test:coverage": "ng test --code-coverage --browsers=ChromeHeadless",
    "test:coverage-report": "npm run test:coverage && http-server -c-1 -o -p 9875 ./coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ci": "npm run lint && npm run build:prod && npm run test:coverage"
  },
  "dependencies": {
    "@angular/animations": "^9.0.7",
    "@angular/common": "^9.0.7",
    "@angular/compiler": "^9.0.7",
    "@angular/core": "^9.0.7",
    "@angular/forms": "^9.0.7",
    "@angular/platform-browser": "^9.0.7",
    "@angular/platform-browser-dynamic": "^9.0.7",
    "@angular/router": "^9.0.7",
    "core-js": "^3.6.4",
    "rxjs": "~6.5.4",
    "tslib": "^1.11.1",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.900.7",
    "@angular/cli": "^9.0.7",
    "@angular/compiler-cli": "^9.0.7",
    "@angular/language-service": "^9.0.7",
    "@types/jasmine": "^3.5.9",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.30",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^5.0.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "^3.1.1",
    "karma-jasmine-html-reporter": "^1.5.2",
    "protractor": "^5.4.3",
    "ts-node": "~8.6.2",
    "tslint": "~5.20.1",
    "typescript": "~3.7.5"
  }
}
